@using Microsoft.Extensions.Localization
@inject IStringLocalizer<SharedResource> L

@{
    // AccessDenied pagina: toont een vertaalde melding wanneer een gebruiker geen rechten heeft voor een pagina.
    ViewData["Title"] = L["Ac_Title"];
}

<h1>@L["Ac_H1"]</h1>
<p class="text-muted">@L["Ac_Text"]</p>

<a asp-controller="Home" asp-action="Index" class="btn btn-primary">@L["Ac_Btn_Home"]</a>
